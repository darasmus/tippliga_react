{
  "name": "cellular-react-redux-boilerplate",
  "version": "0.0.1",
  "description": "cellular-react-redux-boilerplate",
  "repository": {
    "type": "git",
    "url": "gitlab@git.cellular.de:web/cellular_react_redux_boilerplate.git"
  },
  "author": [
    "darasmus <darasmus@cellular.de>",
    "gtavonius <gtavonius@cellular.de>"
  ],
  "license": "UNLICENSED",
  "private": true,
  "main": "src/js/main.js",
  "browserify": {
    "transform": [
      [
        "babelify",
        {
          "ignore": [
            "**/*/node_modules/**/*",
            "**/*/src/js/lib/*"
          ]
        }
      ],
      "envify"
    ]
  },
  "dependencies": {
    "babel-cli": "^6.16.0",
    "babel-core": "^6.17.0",
